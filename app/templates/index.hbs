<h2>Most Recent Questions:</h2>
<div class="row">
  <div class="col-md-6">
    {{query-list queries=model.queries}}
  </div>
  <div class="col-md-6">
    <h3>Tags:</h3>
    <div class="well well-sm">
      {{#each model.tags as |tag|}}
        {{#link-to 'tagged' tag.id}} {{tag-cloud tag}} {{/link-to}}
      {{/each}}
    </div>
    {{query-new newQuery="addQuery"}}
  </div>
</div>
